{
  "name": "voco",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"npm:vite:dev\" \"npm:electron:dev\"",
    "vite:dev": "vite",
    "electron:dev": "electron . --dev",
    "build": "vite build",
    "start": "electron . --enable-features=NetworkService",
    "rebuild-iohook": "npm rebuild iohook --update-binary",
    "server:dev": "ts-node server/server.ts",
    "server:start": "node dist/server/server.js",
    "build:server": "tsc -p tsconfig.server.json",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^26.0.12",
    "node-abi": "^3.51.1",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.8.0",
    "@hurdlegroup/robotjs": "^0.12.3",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "electron-store": "^10.0.1",
    "express": "^4.19.2",
    "lucide-react": "^0.513.0",
    "node-global-key-listener": "^0.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "build": {
    "appId": "com.example.voco",
    "mac": {
      "target": "dmg"
    }
  }
}
